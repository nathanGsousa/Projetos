dados <- read_excel("Dados.xls")
str(dados)
dados <- dados %>%
mutate(
# Converter Data de "2012.01" para objeto Date (primeiro dia de cada mês)
Data = ym(str_replace(Data, "\\.", "-")),
# Converter expectativas de IPCA para numérico
